# NextAuth.js Configuration
AUTH_SECRET=
NEXTAUTH_URL=http://localhost:3000

# JWT Signing Keys (RS256) - Generate with: node scripts/generate-keys.js
NEXTAUTH_JWT_PRIVATE_KEY=
NEXTAUTH_JWT_PUBLIC_KEY=

# Google OAuth
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=

# GitHub OAuth
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=

# Dynamic Keys
DYNAMIC_ENVIRONMENT_ID=
DYNAMIC_AUTH_TOKEN=

# Dynamic Delegated Access Webhook
# Webhook secret for verifying webhook signatures from Dynamic
DYNAMIC_WEBHOOK_SECRET=

# RSA private key for decrypting delegated materials (base64 encoded PEM)
DELEGATION_PRIVATE_KEY=

# Storage encryption key for delegated materials (32 bytes, base64 encoded)
# Generate with: openssl rand -base64 32
STORAGE_ENCRYPTION_KEY=

# BaseScan API (for transaction history - server-side only)
BASESCAN_API_KEY=